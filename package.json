{
  "name": "spell-editor-to-cmangos",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "dotenv -e .env -- tsx src/index.ts",
    "tsc": "tsc --noEmit",
    "db:codegen": "dotenv -e .env -- npm run db:codegen:editor && dotenv -e .env -- npm run db:codegen:cmangos",
    "db:codegen:editor": "kysely-codegen --dialect sqlite --url \"${EDITOR_DB_PATH}/SpellEditor.db\" --out-file ./src/db/editor.ts",
    "db:codegen:cmangos": "kysely-codegen --dialect mysql --url \"${CMANGOS_DB_URL}\" --out-file ./src/db/cmangos.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "dotenv-cli": "^11.0.0",
    "knex": "^3.1.0",
    "kysely": "^0.28.11",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.1.0",
    "kysely-codegen": "^0.19.0",
    "mysql2": "^3.16.2",
    "typescript": "^5.9.3"
  }
}
